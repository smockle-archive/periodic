{
  "name": "@smockle/periodic",
  "version": "2.0.3",
  "description": "Convert monthly price data to monthly return data",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "bin": {
    "periodic": "bin/index.js"
  },
  "scripts": {
    "start": "node -r @std/esm bin/index.mjs",
    "debug": "node --nolazy --inspect-brk=5858 -r @std/esm bin/index.mjs",
    "test": "node -r @std/esm test/index.mjs | colortape",
    "coverage": "nyc --require @std/esm --extension .mjs --reporter=lcov node test/index.mjs | colortape",
    "postcoverage": "nyc report",
    "prepublishOnly": "npm test",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/periodic.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/periodic/issues"
  },
  "homepage": "https://github.com/smockle/periodic#readme",
  "devDependencies": {
    "codecov": "^3.0.0",
    "colortape": "^0.1.2",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nyc": "^11.1.0",
    "prettier-eslint-cli": "^4.3.0"
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  },
  "@std/esm": {
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "^0.18.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "xlsx": "^0.11.3"
  }
}
